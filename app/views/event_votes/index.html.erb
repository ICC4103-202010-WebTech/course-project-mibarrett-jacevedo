<p id="notice"><%= notice %></p>

<h1>Event Votes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_votes.each do |event_vote| %>
      <tr>
        <td><%= link_to 'Show', event_vote %></td>
        <td><%= link_to 'Edit', edit_event_vote_path(event_vote) %></td>
        <td><%= link_to 'Destroy', event_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Vote', new_event_vote_path %>
