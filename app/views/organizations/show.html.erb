<p id="notice"><%= notice %></p>

<h1><%= @organization.name %></h1>


<h3>Events</h3>

<% @events.each do |event| %>

  <div class="dropdown">
    <button class="dropbtn"><%= event.title %></button>
    <div class="dropdown-content">
      <a><%= link_to 'Open Event', event %></a>
    </div>
  </div>
<% end %>

<div class="welcome"> Hi! We are glad you visited our organization website </div>

<br>
<%= link_to 'See all organizations', '/organizations' %>
<%= link_to 'Back', '/events' %>
